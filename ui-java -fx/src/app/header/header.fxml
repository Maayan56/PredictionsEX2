<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane fx:id="headerComponent" stylesheets="@header.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.header.HeaderController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="70.0" prefWidth="200.0">
         <children>
            <Button id="load-file-button" fx:id="loadFileButton" mnemonicParsing="false" onAction="#onLoadFileButtonClick" text="Load File Button" textFill="#f5f5f5">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <TextField prefWidth="300.0">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </TextField>
            <Button id="queue-man-button" fx:id="queueManaementButton" mnemonicParsing="false" onAction="#onQueueManagementButtonClick" prefHeight="80.0" text="Queue Management">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Button>
         </children>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </HBox>
      <HBox prefHeight="45.0" prefWidth="600.0" GridPane.rowIndex="1">
         <children>
            <Button id="details-button" fx:id="detailsButton" mnemonicParsing="false" onAction="#onDetailsButtonClick" prefWidth="100.0" styleClass="move-to-page-button" text="Details">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button id="new-execution-button" fx:id="newExecutionButton" mnemonicParsing="false" onAction="#onNewExecutionButtonClick" styleClass="move-to-page-button" text="New Execution">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
            <Button id="results-button" fx:id="resultButtonClick" mnemonicParsing="false" onAction="#onResultButtonClick" styleClass="move-to-page-button" text="Results" />
         </children>
         <padding>
            <Insets left="140.0" />
         </padding>
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
      </HBox>
   </children>
</GridPane>
